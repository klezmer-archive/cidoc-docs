        <tr class="entity">
            <td>
                <table style="border: none;">
                    <tbody>
                        <tr>
                            <td colspan="3" class="entity-first-cell"><span class="cls" id="TX9">TX9 Glyph<a
                                        href="javascript:void(0)" class="refLink" id="refIdLinkForTX9"
                                        title="Copy reference link to clipboard" onclick="copyToClipboard('TX9');"><i
                                            class="fas fa-link"> </i>
